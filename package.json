{
  "name": "allgood-frontend",
  "version": "1.0.0",
  "homepage": "./",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --passWithNoTests",
    "eject": "craco eject",
    "lint": "npm run prettier:f && npm run lint:f && npm run slint:f",
    "prettier:f": "prettier --write .",
    "lint:f": "eslint --fix --ext .js,.jsx,.ts,.tsx src/ --no-cache",
    "slint:f": "stylelint --fix \"src/**/*.{css,scss}\"",
    "prepare": "husky install",
    "postinstall": "husky install",
    "pre-commit": "npx --no-install lint-staged"
  },
  "dependencies": {
    "@emotion/react": "11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.2",
    "@mui/base": "^5.0.0-beta.4",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "5.13.3",
    "@mui/styled-engine-sc": "^5.12.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react-icons": "^3.0.0",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dotenv": "^16.1.1",
    "formik": "^2.4.0",
    "loadsh": "^0.0.4",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^12.3.1",
    "react-icons": "^4.9.0",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.11",
    "uuid": "^9.0.0",
    "web-vitals": "^3.3.0"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "autoprefixer": "^10.4.14",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "stylelint": "^15.3.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.6.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/index.tsx",
      "!src/react-app-env.d.ts",
      "!src/util/web-vitals.ts",
      "!src/app/index.tsx"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write .",
      "eslint --fix --ext .js,.jsx,.ts,.tsx src/ --no-cache",
      "git add ."
    ],
    "*.{css,scss}": [
      "prettier --write .",
      "stylelint --fix \"src/**/*.{css,scss}\"",
      "git add ."
    ],
    "*.{json,md,html}": [
      "prettier --write .",
      "git add ."
    ]
  },
  "engines": {
    "node": "^18.0.0",
    "yarn": "^3.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
